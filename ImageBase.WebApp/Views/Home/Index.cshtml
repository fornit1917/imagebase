@model List<string>
@addTagHelper "*, Microsoft.AspNetCore.Mvc.TagHelpers"
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="d-flex flex-column align-items-center" id="index">
    <div><p>You are <em>@User.Identity.Name</em>.</p></div>
    <div>
        <p>BTW as a @foreach (var role in Model)
        {<span>@role</span>}. You can sign out</p>

    </div>
        <a href="/api/Authentication/logout">Sign out</a>
    </div>
    
</div>